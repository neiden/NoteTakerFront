{
  "routes": [
    {
      "route": "/login",
      "serve": "/login.html",
      "allowedRoles": ["anonymous", "authenticated"]
    },
    {
      "route": "/*",
      "serve": "/login.html",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/login.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*"]
  },
  "responseOverrides": {
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "redirect": "/login",
      "statusCode": 302
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self'; connect-src 'self'  https://localhost:7089 https://notetakerbackend.azurewebsites.net; script-src 'self'; style-src 'self' 'unsafe-inline'; img-src 'self' data:; font-src 'self';"
  }
}
